rule "1 πηγή παντοθενικού οξέως"
    when
        $p : DishBean($pantothenic_NRV : pantothenic_NRV,
        	$pantothenic_NRV >= 0.15 && $pantothenic_NRV < 0.3)               
    then
    	$p.getExplanations().put("Tο προϊόν είναι πηγή παντοθενικού οξέως.", 1.0)
        System.out.println("Rule πηγή παντοθενικού οξέως ");    
end

