rule "1 πηγή βιταμίνης A"
    when
        $p : DishBean($vitamin_A_NRV : vitamin_A_NRV,
        	$vitamin_A_NRV >= 0.15 && $vitamin_A_NRV < 0.3)               
    then
    	$p.getExplanations().put("Tο προϊόν είναι πηγή βιταμίνης A.", 1.0)
        System.out.println("Rule πηγή βιταμίνης A ");    
end

